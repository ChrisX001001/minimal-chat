    "@fortawesome/fontawesome-free": "^6.4.0",
    "@google-ai/generativelanguage": "^0.2.1",
    "del": "^7.0.0",
    "dotenv": "^16.0.3",
    "google-auth-library": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-html-replace": "^1.6.2",
    "gulp-inject-string": "^1.1.2",
    "gulp-load-plugins": "^2.0.8",
    "gulp-newer": "^1.4.0",
    "http-server": "^14.1.1",
    "knockout": "^3.5.1",
    "markdown": "^0.5.0",
    "markdown-it-highlight": "^0.2.0",
